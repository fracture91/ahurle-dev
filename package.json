    "@babel/core": "^7.12.16",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.4",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.7",
    "@theme-ui/color-modes": "^0.6.0-alpha.6",
    "fecha": "^4.2.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "hast-util-shift-heading": "^2.0.1",
    "image-size": "^0.9.3",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "mdx-prism": "https://github.com/doamatto/mdx-prism/archive/v0.3.2.tar.gz",
    "next": "^10.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "raw-loader": "^4.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "reading-time": "^1.3.0",
    "rebass": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.0.tgz",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-slug": "^4.0.1",
    "remark-directive": "^1.0.1",
    "rss": "^1.2.2",
    "rundef": "^1.2.6",
    "showdown": "^1.9.1",
    "theme-ui": "^0.6.0-alpha.6",
    "unist-builder": "^2.0.3",
    "unist-util-visit": "^2.0.3",
    "utility-types": "^3.10.0",
    "zod": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.1",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/rss": "^0.0.28",
    "@types/showdown": "^1.9.3",
    "@types/unist": "^2.0.3",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "bundlewatch": "^0.3.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "favicons": "^6.1.0",
    "jest": "^26.6.3",
    "jest-fail-on-console": "^2.0.3",
    "jest-raw-loader": "^1.0.1",
    "jest-transformer-mdx": "^2.2.0",
    "next-page-tester": "^0.21.0",
    "patch-package": "^6.2.2",
    "prettier": "2.2.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.2.2"
  },
  "resolutions": {
    "refractor": "^3.3.1",
    "react": "^17.0.1",
    "rebass": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.0.tgz",
    "@babel/core": "^7.12.16",
    "babel-preset-react-app": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.0.tgz"
  },
  "bundlewatch": {
    "ci": {
      "trackBranches": [
        "master"
      ]
    },
    "normalizeFilenames": "^.+?((\\.|-)[a-f0-9]{20})?\\.(?:js|css|html)$",
    "files": [
      {
        "path": "./.next/static/chunks/**/!(polyfills|react-syntax-highlighter_languages_)*.js",
        "maxSize": "80kb"
      },
      {
        "path": "./.next/static/chunks/main-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/webpack-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/framework.*.js",
        "maxSize": "50kb"
      },
      {
        "path": "./.next/static/chunks/commons.*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/polyfills-*.js",
        "maxSize": "40kb"
      },
      {
        "path": "./.next/static/*/pages/**/*.js",
        "maxSize": "20kb"
      },
      {
        "path": "./.next/static/chunks/pages/_@(app|error)-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/pages/index-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/*/_buildManifest.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/server/pages/**/*.html",
        "maxSize": "50kb"
      }
    ]
  }
}
