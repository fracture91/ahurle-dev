  "tags": [
    "blog",
    "react",
    "nextjs",
    "syntax highlighting",
    "static generation"
  ],
  "keywords": [
    "blog",
    "react",
    "nextjs",
    "syntax highlighting",
    "static generation"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "postinstall": "patch-package",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "export": "next export",
    "lint": "yarn lint:eslint; yarn lint:prettier",
    "lint:eslint": "yarn run eslint . --ext .js,.ts,.jsx,.tsx,.mdx --max-warnings 0",
    "lint:prettier": "yarn run prettier --check .",
