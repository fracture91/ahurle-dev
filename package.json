{
  "name": "ahurle-dev",
  "version": "0.5.0",
  "description": "ahurle.dev blog site, based on colinhacks/devii",
  "repository": {
    "type": "git",
    "url": "https://github.com/fracture91/ahurle-dev"
  },
  "author": "Andrew Hurle <adhurle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fracture91/ahurle-dev/issues"
  },
  "homepage": "https://github.com/fracture91/ahurle-dev",
  "tags": [
    "blog",
    "react",
    "nextjs",
    "syntax highlighting",
    "static generation"
  ],
  "keywords": [
    "blog",
    "react",
    "nextjs",
    "syntax highlighting",
    "static generation"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export"
  },
  "dependencies": {
    "fecha": "^4.2.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "next": "^10.0.5",
    "raw-loader": "^4.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "rss": "^1.2.2",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@types/glob": "^7.1.3",
    "@types/node": "14",
    "@types/react": "^17.0.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/rss": "^0.0.28",
    "@types/showdown": "^1.9.3",
    "bundlewatch": "^0.3.1",
    "favicons": "^6.1.0",
    "typescript": "^4.1.3"
  },
  "bundlewatch": {
    "trackBranches": [
      "master"
    ],
    "normalizeFilenames": "^.+?((\\.|-)[a-f0-9]+)\\.(?:js|css)$",
    "files": [
      {
        "path": "./.next/static/chunks/**/!(polyfills|react-syntax-highlighter_languages_)*.js",
        "maxSize": "80kb"
      },
      {
        "path": "./.next/static/chunks/main-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/webpack-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/framework.*.js",
        "maxSize": "50kb"
      },
      {
        "path": "./.next/static/chunks/commons.*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/polyfills-*.js",
        "maxSize": "40kb"
      },
      {
        "path": "./.next/static/*/pages/**/*.js",
        "maxSize": "20kb"
      },
      {
        "path": "./.next/static/chunks/pages/_@(app|error)-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/pages/index-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/chunks/pages/blog/\\[blog\\]-*.js",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/static/css/**/*.css",
        "maxSize": "20kb"
      },
      {
        "path": "./.next/static/*/_buildManifest*",
        "maxSize": "10kb"
      },
      {
        "path": "./.next/server/pages/**/*.html",
        "maxSize": "50kb"
      }
    ]
  }
}
